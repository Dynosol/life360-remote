# Life360 API Configuration

# API Base URL
LIFE360_BASE_URL=https://api-cloudfront.life360.com

# Default country code
LIFE360_COUNTRY_CODE=1

# Circle ID (leave empty to use first circle)
LIFE360_CIRCLE_ID=

# Verbose logging (True/False)
LIFE360_VERBOSE=True

# Device ID
LIFE360_DEVICE_ID=1234

# User Agent
LIFE360_USER_AGENT=XYZ/ABC/123

# Basic Auth Token
LIFE360_AUTH_BASIC=Basic XYZ==
